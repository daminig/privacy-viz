{
  "name": "Privacy Visualizer",
  "description": "Visualize your privacy security.",
  "version": "1.0",

  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "<all_urls>",
    "http://localhost/*",
    "cookies",
    "history"
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Privacy Visualizer",
    "default_popup": "popup.html"
  },

  "manifest_version": 2
}
